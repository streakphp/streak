{
  "name": "streak/streak",
  "type": "library",
  "description": "",
  "keywords": ["es","cqrs","event sourcing", "command query responsibility segregation", "command", "command bus", "event"],
  "homepage": "https://streakphp.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Alan Gabriel Bem",
      "email": "alan.bem@gmail.com",
      "homepage": "https://github.com/alanbem"
    }
  ],
  "autoload": {
    "psr-4": {
      "Streak\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Streak\\": "tests/"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "require": {
    "php": ">=7.1",
    "ext-PDO": "^7.1",
    "ext-igbinary": "^2.0|^3.0",
    "ext-redis": "^4.0|^5.0",
    "doctrine/dbal": "^2.8",
    "psr/log": "~1.0",
    "ramsey/uuid": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "friendsofphp/php-cs-fixer": "^2.9",
    "php-amqplib/rabbitmq-bundle": "^1.14"
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
